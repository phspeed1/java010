{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/**.class": true
    }, 
    "code-runner.executorMap": {
        "java": "cd $dir && javac $fileName -encoding utf-8 && java $fileNameWithoutExt"
    }
}